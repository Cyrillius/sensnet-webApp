 {
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
        "include": ["./"],
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "docs"
    },
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"cleverLinks": true,
		"monospaceLinks": true,
 		"default": {
 			"outputSourceFiles": true
 		},
		"systemName"      : "DocStrap",
		"systemName"      : "Sensnet",
 		"footer"          : "",
 		"copyright"       : "DocStrap Copyright.",
 		"navType"         : "vertical",
		"theme"           : "journal",
		"theme"           : "cerulean",
 		"linenums"        : true,
 		"collapseSymbols" : false,
 		"inverseNav"      : true
	},
	"plugins": [ "plugins/markdown" ],
	"markdown": {
		"parser": "gfm",
		"hardwrap": true
 	}
 }